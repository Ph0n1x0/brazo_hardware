<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>brazo_moveit_config</name>
  <version>2023.10.6</version>
  <description>Package for using Moveit 2 with the brazo robot</description>

  <author     email="lmloaizaz@eafit.edu.co">Luis Miguel Loaiza</author>
  <maintainer email="lmloaizaz@eafit.edu.co">Luis Miguel Loaiza</maintainer>

  <license>GPL-3.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
    <gazebo_ros gazebo_model_path = "${prefix}/.."/>
  </export>
</package>